    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/remirror__extension-font-size",
  "repository": {
    "type": "git",
    "url": "https://github.com/remirror/remirror.git",
    "directory": "packages/remirror__extension-font-size"
  },
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/rme-sdk-extension-font-size.js",
      "require": "./dist/rme-sdk-extension-font-size.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/rme-sdk-extension-font-size.cjs",
  "module": "./dist/rme-sdk-extension-font-size.js",
